<% include templete/header.ejs %>
  
    <div class="container">
        <div class="jumbotron text-center">
            <h1 class="display-5"><%= title %> </h1>
            <p class="lead">Sửa dữ liệu trong MongoDb</p>
            <hr class="my-2">
            
        </div>
    <% data.forEach( fixct => { %>
    <form action="/fixdl/<%=fixct._id %>" method="post">
    
        <div class="form-group">
          <label for="name">Tên</label>
          <input type="text" name="namedb" id="" class="form-control" placeholder="" aria-describedby="helpId" value="<%=fixct.name %>">
          <small id="helpId" class="text-muted">Mời bạn điền đầy đủ họ tên vào đây</small>
        </div>

        <div class="form-group">
          <label for="phone">Tuổi</label>
          <input type="text"
            class="form-control" name="agedb" id="" aria-describedby="helpId" placeholder="" value="<%=fixct.age %>">
          <small id="helpId" class="form-text text-muted">Mời bạn điền số tuổi vào đây</small>
        </div>
    
        <button type="submit" class="btn btn-primary">Gửi</button>

    </form><%}); %>
    </div>







    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.2/js/bootstrap.min.js" integrity="sha384-o+RDsa0aLu++PJvFqy8fFScvbHFLtbvScb8AjopnFD+iEQ7wo/CG0xlczd+2O/em" crossorigin="anonymous"></script>
  </body>
</html>